# function to read tau data and merge with covariates
readtaudata <- function(fname, covariates=covada) {
  da <- readpetdata(fname)
  # Remove incomplete (erroneous) entries
  da <- da[complete.cases(da),]
  # Merge tau data with covariate data
  m <- merge(da, covariates, by=c("blsaid","blsavi"), all.x=T)
  m <- m[order(m$blsaid,m$blsavi),]
  
  m
}

