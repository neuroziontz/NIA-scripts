readpetdata <- function(fname) {
  da <- as.data.frame(read_excel(fname, skip=1))
  colnames(da) <- make.names(colnames(da), unique=T)
  da$blsaid <- as.numeric(substr(da$idvi,1,4))
  da$blsavi <- as.numeric(substr(da$idvi,6,7)) + as.numeric(substr(da$idvi,9,9))/10
  da
}

